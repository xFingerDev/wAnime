{
  "expo": {
    "name": "wAnime",
    "slug": "wANIME",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "animesource",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      },
      "bundleIdentifier": "com.communitybakery.wanime"
    },
    "android": {
      "permissions": [
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECORD_AUDIO",
        "com.google.android.gms.permission.AD_ID"
      ],
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.communitybakery.wanime",
      "runtimeVersion": "1.0.1"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "extraProguardRules": "-keep class com.google.android.gms.internal.consent_sdk.** { *; }"
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4713105116292090~7072133863",
          "iosAppId": "ca-app-pub-4713105116292090~7072133863",
          "delayAppMeasurementInit": true,
          "userTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3fe9aade-e82b-4f98-8fb3-226a4bffeb9f"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/3fe9aade-e82b-4f98-8fb3-226a4bffeb9f"
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-4713105116292090~7072133863",
    "ios_app_id": "ca-app-pub-4713105116292090~7072133863"
  }
}
